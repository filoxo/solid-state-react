{
  "name": "solid-state-react",
  "description": "HTML5 UP's Solid State theme enhanced with React.",
  "author": "Carlos Filoteo (@filoxo)",
  "repository": {
    "type": "git",
    "url": "https://github.com/filoxo/solid-state-react.git"
  },
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "font-awesome": "^4.7.0",
    "lodash.throttle": "^4.1.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-focus-trap": "^2.6.0",
    "react-modal": "^3.6.1",
    "react-scripts": "2.0.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "format": "prettier src/**/*.js --write --no-semi --single-quote --trailing-comma es5 --tab-width 2 --parser flow"
  },
  "devDependencies": {
    "flow-bin": "^0.82.0",
    "husky": "^1.1.1",
    "lint-staged": "^7.3.0",
    "react-testing-library": "^5.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
